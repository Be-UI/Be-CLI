{
  "name": "be-cli",
  "type": "module",
  "version": "1.0.0",
  "private": false,
  "description": "a project template cli",
  "author": "baiwusanyu-c",
  "license": "MIT",
  "keywords": [
    "vue3",
    "react",
    "eslint",
    "commitlint",
    "prettier"
  ],
  "bin": {
    "be-cli": "entry/index.js"
  },
  "files": [
    "dist",
    "entry"
  ],
  "scripts": {
    "init": "pnpm i",
    "lint:fix": "eslint . --fix",
    "dev": "cross-env ENV=BECLIDEV esno core/index.ts",
    "build": "rollup -c build-script/rollup.config.js",
    "prepublishOnly": "pnpm run build",
    "release": "pnpm -r publish --access public --no-git-checks"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "colors": "^1.4.0",
    "commander": "^9.0.0",
    "fs-extra": "^10.0.1",
    "ora": "^6.1.2",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@baiwusanyu/eslint-config": "^1.0.6",
    "@changesets/cli": "^2.23.1",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/chalk": "^2.2.0",
    "@types/commander": "^2.12.2",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.0.0",
    "@types/prompts": "^2.0.14",
    "cross-env": "^7.0.3",
    "eslint": "^8.18.0",
    "esno": "^0.16.3",
    "pnpm": "^6.3.0",
    "rollup": "^2.73.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.33.0",
    "tslib": "^2.3.1",
    "turbo": "^1.2.16",
    "typescript": "4.7.4",
    "unbuild": "^0.7.4",
    "vite": "^2.9.12",
    "vitest": "^0.15.2"
  },
  "eslintConfig": {
    "extends": [
      "@baiwusanyu"
    ]
  }
}
