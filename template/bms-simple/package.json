  "repository": {
    "type": "git",
    "url": "http://192.168.110.101:8081/bigdata/frontend/ab-test-vue.git"
  },
  "module": "esnext",
  "scripts": {
    "init": "pnpm i",
    "serve:dev": "vite --host --mode development",
    "serve:qa": "vite --host --mode staging",
    "serve:prod": "vite --host --mode production",
    "build:prod": "vite build --mode production",
    "build:dev": "vite build --mode development",
    "build:qa": "vite build --mode staging",
    "lint:ts-fix": "eslint --fix src --ext .vue,.js,.ts,.jsx,.tsx ",
    "prepare": "husky install",
    "husky:step1": "npx husky add .husky/pre-commit \"npm exec lint-staged --allow-empty\"",
    "husky:step2": "git add .husky/pre-commit && npx husky add .husky/commit-msg \"npx --no -- commitlint --edit $1\" && git add .husky/commit-msg",
    "preview": "vite preview",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "release:tag": "bumpp package.json --commit --push --tag"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.10",
    "@vueuse/core": "^8.5.0",
    "axios": "^0.26.1",
    "echarts": "^5.3.3",
    "element-plus": "^2.2.19",
    "file-saver": "^2.0.5",
    "fuse.js": "^6.5.3",
    "js-cookie": "^3.0.1",
    "jsencrypt": "^3.2.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.14",
    "pnpm": "^7.9.0",
    "vue": "^3.2.45",
    "vue-cropper": "^1.0.3",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.16.0",
    "@baiwusanyu/eslint-config": "^1.0.10",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/file-saver": "^2.0.5",
    "@types/js-cookie": "^3.0.2",
    "@types/randomcolor": "^0.5.6",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/compiler-sfc": "^3.2.45",
    "bumpp": "^8.2.1",
    "chalk": "4.1.2",
    "chokidar": "^3.5.3",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.18.0",
    "fs-extra": "^10.0.1",
    "husky": "^7.0.4",
    "ora": "^6.1.2",
    "randomcolor": "^0.6.2",
    "sass": "^1.52.1",
    "tinify": "^1.7.1",
    "typescript": "^4.1.3",
    "unconfig": "^0.3.5",
    "unplugin-auto-import": "^0.8.5",
    "unplugin-img-compress": "^1.1.0",
    "vite": "4.0.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vue-tsc": "^1.0.24"
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx,md,json}": [
      "eslint ./ --fix",
      "git add"
    ]
  }
}
